#Find the number that appears once, and the other numbers twice
'''xor - when we xor a number even number of times it returns 0
and when we xor a number odd number of time it returns the num itself'''

def getSingleElement(arr):
    # XOR all the elements:
    xorr = 0
    for num in arr:
        xorr ^= num
        
    return xorr


arr = [4, 1, 2, 1, 2]
ans = getSingleElement(arr)
print("The single element is:", ans)
#t.c o(n)


# Find Missing no from array
arr1 = [3,0,1]   
n = len(arr1)+1     

sumn=0
sum  = n*(n+1)//2

for i in range(len(arr1)):
    sumn +=arr1[i]
print(sum-sumn)

#t.c o(n)

hash = {}
def getone(arr):
    for i in range(len(arr)):
        hash[arr[i]] = hash.get(arr[i],0)+1
    
    for k,v in hash.items():
        if v==1:
            return k
            
print(getone(arr))
