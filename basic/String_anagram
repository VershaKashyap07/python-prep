#Given two strings, write a function to check if one string is an anagram of the other (contains the same characters in a different order)
str1 ='listen'
str2 = 'slient'

def anagram(str1,str2):
    if len(str1)!= len(str2):
        return False
    dict ={}
    for i in str1:
        if i in dict:
            dict[i]+=1
        else:
            dict[i]=1
        
    for j in str2:
        if j in dict:
            dict[j]-=1
        else:
            return False

    for count in dict.values():
        if count !=0:
            return False
    return True

print(anagram(str1,str2))